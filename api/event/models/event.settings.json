{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "name": "Event",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "submissionEndDate": {
      "type": "datetime",
      "required": false
    },
    "coverImage": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "isTeamEvent": {
      "type": "boolean",
      "required": true
    },
    "category": {
      "autoPopulate": false,
      "model": "category",
      "via": "events"
    },
    "contacts": {
      "type": "component",
      "repeatable": true,
      "component": "contact.contact"
    },
    "regStartDate": {
      "type": "datetime",
      "required": true
    },
    "regEndDate": {
      "type": "datetime",
      "required": true
    },
    "minTeamSize": {
      "type": "integer"
    },
    "maxTeamSize": {
      "type": "integer",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "isSubmissionEvent": {
      "type": "boolean",
      "required": true
    },
    "metaTitles": {
      "type": "json"
    },
    "description": {
      "type": "text"
    },
    "rules": {
      "type": "text"
    },
    "result": {
      "type": "text"
    },
    "posterImage": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "submissionStartDate": {
      "type": "datetime"
    },
    "maxFileSize": {
      "type": "integer"
    }
  }
}
