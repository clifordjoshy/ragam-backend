{
  "kind": "collectionType",
  "collectionName": "user_event_details",
  "info": {
    "name": "UserEventDetail",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "event": {
      "autoPopulate": false,
      "model": "event"
    },
    "submissions": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "teamMembers": {
      "plugin": "users-permissions",
      "collection": "user",
      "via": "eventDetails"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "participating",
        "knocked_out",
        "position_1",
        "position_2",
        "position_3",
        "consolation"
      ]
    },
    "eventMeta": {
      "type": "component",
      "repeatable": true,
      "component": "event-meta.event-meta"
    }
  }
}
